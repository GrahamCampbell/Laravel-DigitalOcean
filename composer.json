{
    "name": "graham-campbell/digitalocean",
    "description": "DigitalOcean Is A DigitalOcean Bridge For Laravel",
    "keywords": ["laravel", "framework", "digitalocean", "digitalocean-v2", "DigitalOceanV2", "digitalocean bridge", "bridge", "DigitalOcean", "Laravel DigitalOcean", "Laravel-DigitalOcean", "Graham Campbell", "GrahamCampbell"],
    "license": "MIT",
    "authors": [
        {
            "name": "Graham Campbell",
            "email": "graham@alt-three.com"
        }
    ],
    "require": {
        "php": "^7.2.5 || ^8.0",
        "graham-campbell/manager": "^4.5",
        "illuminate/contracts": "^6.0 || ^7.0 || ^8.0",
        "illuminate/support": "^6.0 || ^7.0 || ^8.0",
        "toin0u/digitalocean-v2": "3.2.*"
    },
    "require-dev": {
        "graham-campbell/analyzer": "^3.0",
        "graham-campbell/testbench": "^5.4",
        "guzzlehttp/guzzle" : "^7.0",
        "kriswallsmith/buzz": "^0.16",
        "mockery/mockery": "^1.3.1",
        "phpunit/phpunit": "^8.5.8 || ^9.3.7"
    },
    "suggest": {
        "kriswallsmith/buzz": "Enables the buzz connector.",
        "guzzlehttp/guzzle": "Enables the guzzlehttp connector."
    },
    "autoload": {
        "psr-4": {
            "GrahamCampbell\\DigitalOcean\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GrahamCampbell\\Tests\\DigitalOcean\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "laravel": {
            "providers": [
                "GrahamCampbell\\DigitalOcean\\DigitalOceanServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
