{
    "name": "graham-campbell/digitalocean",
    "description": "DigitalOcean Is A DigitalOcean Bridge For Laravel",
    "keywords": ["laravel", "framework", "digitalocean", "digitalocean-v2", "DigitalOceanV2", "digitalocean bridge", "bridge", "DigitalOcean", "Laravel DigitalOcean", "Laravel-DigitalOcean", "Graham Campbell", "GrahamCampbell"],
    "license": "MIT",
    "authors": [
        {
            "name": "Graham Campbell",
            "email": "hello@gjcampbell.co.uk",
            "homepage": "https://github.com/GrahamCampbell"
        }
    ],
    "require": {
        "php": "^7.4.15 || ^8.0.2",
        "graham-campbell/manager": "^5.1",
        "guzzlehttp/guzzle": "^7.8.1",
        "guzzlehttp/psr7": "^2.6.2",
        "illuminate/contracts": "^8.75 || ^9.0 || ^10.0 || ^11.0",
        "illuminate/support": "^8.75 || ^9.0 || ^10.0 || ^11.0",
        "toin0u/digitalocean-v2": "4.9.*"
    },
    "require-dev": {
        "graham-campbell/analyzer": "^4.1",
        "graham-campbell/testbench": "^6.1",
        "mockery/mockery": "^1.6.6",
        "phpunit/phpunit": "^9.6.15 || ^10.4.2"
    },
    "autoload": {
        "psr-4": {
            "GrahamCampbell\\DigitalOcean\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GrahamCampbell\\Tests\\DigitalOcean\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "GrahamCampbell\\DigitalOcean\\DigitalOceanServiceProvider"
            ]
        }
    }
}
